{
  "extends": "astro/tsconfigs/strict",
  "include": [".astro/types.d.ts", "**/*"],
  "exclude": ["dist"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "react",
    "baseUrl": "./src",
    "paths": {
      "@/*": ["./src/*"],
      "@img/*": ["assets/img/*"],
      "@components/*": ["components/*"],
      "@content/*": ["content/*"],
      "@assets/*": ["assets/*"],
      "@cmsImg/*": ["assets/img/cms/*"]
    }
  }
}
